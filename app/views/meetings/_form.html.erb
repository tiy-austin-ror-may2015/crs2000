  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :agenda %><br>
    <%= f.text_field :agenda %>
  </div>
  <div class="field">
    <%= f.label :room %><br>
    <%= f.select :room, @all_rooms %>
  </div>
  <div class="field">
    <%= f.label :start_time %><br>
    <%= f.datetime_select :start_time %>
  </div>
  <div class="field">
    <br>
    <%= f.datetime_select :end_time %>
  </div>
  <div class="field">
    <%= f.label :private %><br>
    <%= f.check_box :private %>
  </div>
  <div>
    <%= f.hidden_field :employee_id, value: current_employee.id %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
